{
  "$schema": "https://schemas.wp.org/wp/6.8/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "defaultDuotone": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "gradients": [
        {
          "slug": "pink-to-pale-purple",
          "gradient": "linear-gradient(to bottom right,rgba(255,30,142) 49.9%,rgb(229,6,229, 0.36) 50%)",
          "name": "Diagonal pink to pale purple"
        },
        {
          "slug": "footer-gradient",
          "gradient": "linear-gradient(135deg,rgb(26,26,18) 0%,rgb(9,43,85) 100%)",
          "name": "Footer Gradient"
        }
      ],
      "palette": [
        {
          "color": "oklch(0.928 0.006 264.531)",
          "name": "Site Navbar Text",
          "slug": "site-navbar-text"
        },
        {
          "color": "oklch(0.928 0.006 264.531)",
          "name": "Site Title",
          "slug": "site-title"
        },
        {
          "color": "oklch(0.391 0.09 240.876)",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#7456F1",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#fefefe",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#2c2c2c",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "oklch(0.45 0.085 224.283)",
          "name": "Accent",
          "slug": "accent"
        },
        {
          "color": "#4aa775",
          "name": "Accent Light",
          "slug": "accent-light"
        },
        {
          "color": "#363636",
          "name": "Gray",
          "slug": "gray"
        },
        {
          "color": "#2d2d2d",
          "name": "Dark Gray",
          "slug": "dark-gray"
        },
        {
          "color": "#D3D3D3",
          "name": "Light Text",
          "slug": "light-text"
        },
        {
          "color": "oklch(0.24 0.05 254.58 / 0.33)",
          "name": "White Text Color",
          "slug": "white-text-color"
        },
        {
          "color": "oklch(0.391 0.09 240.876)",
          "name": "Button Background",
          "slug": "button-bg"
        },
        {
          "color": "oklch(0.443 0.11 240.79)",
          "name": "Button Background Hover Color",
          "slug": "button-bg-hover"
        },
        {
          "color": "rgba(0,0,0,0.1)",
          "name": "Button Shadow",
          "slug": "button-shadow"
        },
        {
          "color": "#FAFAFA",
          "name": "Button Hover Text ",
          "slug": "button-hover-text"
        },
        {
          "color": "rgba(0, 0, 0, 0.1)",
          "name": "Black Transparent 10",
          "slug": "black-transparent-10"
        },
        {
          "color": "rgba(0, 0, 0, 0.2)",
          "name": "Black Transparent 20",
          "slug": "black-transparent-20"
        },
        {
          "color": "rgba(0, 0, 0, 0.3)",
          "name": "Black Transparent 30",
          "slug": "black-transparent-30"
        },
        {
          "color": "rgba(0, 0, 0, 0.4)",
          "name": "Black Transparent 40",
          "slug": "black-transparent-40"
        },
        {
          "color": "rgba(0, 0, 0, 0.5)",
          "name": "Black Transparent 50",
          "slug": "black-transparent-50"
        },
        {
          "color": "rgba(0, 0, 0, 0.6)",
          "name": "Black Transparent 60",
          "slug": "black-transparent-60"
        }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1200px"
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "size": "1px",
          "slug": "1_px",
          "name": "1_PX"
        },
        {
          "size": "3px",
          "slug": "3_px",
          "name": "3_PX"
        },
        {
          "name": "1",
          "size": "1rem",
          "slug": "10"
        },
        {
          "name": "2",
          "size": "min(1.5rem, 2vw)",
          "slug": "20"
        },
        {
          "name": "3",
          "size": "min(2.5rem, 3vw)",
          "slug": "30"
        },
        {
          "name": "4",
          "size": "min(4rem, 5vw)",
          "slug": "40"
        },
        {
          "name": "5",
          "size": "min(6.5rem, 8vw)",
          "slug": "50"
        },
        {
          "name": "6",
          "size": "min(10.5rem, 13vw)",
          "slug": "60"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "writingMode": true,
      "fontWeight": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "300 900",
              "src": [
                "file:./assets/fonts/inter/Inter-VariableFont_slnt,wght.woff2"
              ]
            }
          ],
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "body"
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.9rem",
          "slug": "small"
        },
        {
          "fluid": false,
          "name": "Medium",
          "size": "1.05rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.39rem",
            "max": "1.85rem"
          },
          "name": "Large",
          "size": "1.85rem",
          "slug": "large"
        },
        {
          "fluid": {
            "min": "1.85rem",
            "max": "2.5rem"
          },
          "name": "Extra Large",
          "size": "2.5rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "min": "2.5rem",
            "max": "3.27rem"
          },
          "name": "Extra Extra Large",
          "size": "3.27rem",
          "slug": "xx-large"
        },
        {
          "fluid": {
            "min": "1rem",
            "max": "7rem"
          },
          "name": "Extra Extra Extra Large",
          "size": "7rem",
          "slug": "xxx-large"
        }
      ]
    },
    "custom": {
      "breakpoint": {
        "small": "360px",
        "medium": "780px",
        "large": "1080px"
      }
    }
  },
  "styles": {
    "blocks": {
      "core/navigation-submenu": {
        "color": {
          "background": "red"
        },
        "spacing": {
          "padding": {
            "top": "20",
            "right": "20",
            "bottom": "20",
            "left": "20"
          }
        }
      },
      "core/navigation": {
        "dimensions": {
          "minHeight": "14px"
        },
        "spacing": {
          "blockGap": "var:preset|spacing|40",
          "margin": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          },
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        },
        "typography": {
          "fontFamily": "var:preset|font-family|body",
          "fontSize": "var(--wp--preset--font-size--sml-medium)"
        },
        "background": {
          "backgroundImage": {
            "url": ""
          }
        },
        "color": {
          "background": "",
          "gradient": "",
          "text": "var(--wp--preset--color--site-navbar-text)"
        }
      },
      "core/paragraph": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--sml-medium)"
        },
        "color": {
          "text": "var(--wp--preset--color--black)"
        }
      },
      "core/read-more": {
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--primary)"
            }
          }
        }
      },
      "core/site-title": {
        "css": "",
        "color": {
          "text": "var(--wp--preset--color--site-title)",
          "background": "none",
          "gradient": "none"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|body",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "300",
          "letterSpacing": "calc(-3px)",
          "textTransform": "none"
        }
      }
    },
    "color": {
      "text": "var:preset|color|contrast",
      "background": "var:preset|color|base"
    },
    "spacing": {
      "padding": {
        "left": "20px",
        "right": "20px",
        "top": "",
        "bottom": ""
      }
    },
    "elements": {
      "button": {
        "css": "transition: 200ms all ease-in-out;",
        "border": {
          "radius": "0px"
        },
        "spacing": {
          "padding": {
            "top": "0.75em",
            "right": "1.25rem",
            "bottom": "0.75em",
            "left": "1.25rem"
          }
        },
        "color": {
          "background": "var(--wp--preset--color--button-bg)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontStyle": "normal",
          "fontWeight": "300",
          "textTransform": "uppercase",
          "letterSpacing": "0.85px",
          "fontFamily": "var:preset|font-family|body"
        },
        ":hover": {
          "shadow": "0px 0px 5px 2px var(--wp--preset--color--button-shadow)",
          "border": {
            "color": "var(--wp--preset--color--button-shadow)",
            "width": "0px",
            "style": "solid"
          },
          "color": {
            "background": "var(--wp--preset--color--button-bg-hover)",
            "text": "var(--wp--preset--color--white)"
          }
        }
      }
    }
  }
}